<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:jqm='urn:import:com.sksamuel.jqm4gwt'
  xmlns:h="urn:import:com.sksamuel.jqm4gwt.html"
  xmlns:b="urn:import:com.sksamuel.jqm4gwt.button"
  xmlns:t="urn:import:com.sksamuel.jqm4gwt.toolbar"
  xmlns:l="urn:import:com.sksamuel.jqm4gwt.list"
  xmlns:ly="urn:import:com.sksamuel.jqm4gwt.layout"
  xmlns:f="urn:import:com.sksamuel.jqm4gwt.form"
  xmlns:e="urn:import:com.sksamuel.jqm4gwt.form.elements"
  xmlns:ta="urn:import:com.sksamuel.jqm4gwt.table"
  xmlns:dt="urn:import:com.sksamuel.jqm4gwt.plugins.datebox"
  xmlns:gr960="urn:import:com.sksamuel.jqm4gwt.plugins.grid960">

  <ui:style>
    .contentBorder { border: red solid 0px; }

    .pageContent {
      margin-left: 1%;
      margin-right: 1%;
      width: 98%;
    }

    .bevel { height: 1em; }

    .red { background-color: red; color: yellow; }
    .green { background-color: green; color: yellow; }
    .blue { background-color: blue; color: yellow; }
    .yellow { background-color: yellow; color: red; }
    .centerText {
      display: block;
      text-align: center;
    }

    .headerImportantColor { color: #ffa200; }

    .inlineBlock {
      display: inline-block;
      vertical-align: middle; /* makes more smooth/uniform line/row */
    }

    .marginRight10 { margin-right: 10px; }
    .marginRight20 { margin-right: 20px; }
    .marginLeft10 { margin-left: 10px; }
    .marginLeft20 { margin-left: 20px; }
    
    .marginTopBottom0 {
      margin-top: 0;
      margin-bottom: 0;
    }

    @external .jqm4gwt-thead-groups, .ui-table-reflow .ui-table-cell-label-top;
    .jqm4gwt-thead-groups th, .ui-table-reflow tbody .ui-table-cell-label-top {
      text-align: center;
      background-color: rgba(0,0,0,.1);
      border-right:1px solid #fff;
    }
    .ui-table-reflow tbody .ui-table-cell-label-top {
      font-weight: bold;
    }
    .jqm4gwt-thead-groups th[data-priority="1"] {
      background-color: rgba(0,0,0,.5);
      color: #fff;
      text-shadow: none;
    }

    .tooltipPop { max-width: 350px; }

    .wordWrap {
      white-space: normal;
    }

  </ui:style>

  <jqm:JQMPage containerId="testPage1" backButton="true"
    contentCentered="true" contentHeightPercent="98.0"
    contentAddStyleNames="jqm4gwt-scrollable-page-content {style.contentBorder}">
    <!-- hideFixedToolbarsIfVirtualKeyboard="250" pseudoFixedToolbars="true" -->

    <t:JQMHeader fixed="true">
      <t:widget>
        <h:Heading.H1 text="Test Page1 Header" />
      </t:widget>
      <t:widget>
        <g:FlowPanel ui:field="headerPanel1">
          <b:JQMButton ui:field="headerTestBtn1" text="Btn1" inline="true" mini="true" />
          <b:JQMButton ui:field="headerTestBtn2" text="Btn2" inline="true" mini="true" />
          <b:JQMButton text="Open Panel" href="#testView1Panel" inline="true" mini="true" />
          <b:JQMButton text="Global Panel" href="#globalPanel1" inline="true" mini="true" />
        </g:FlowPanel>
      </t:widget>
    </t:JQMHeader>

    <!-- jqm:plugins.iscroll.IScrollPanel-->
    <g:FlowPanel addStyleNames="jqm4gwt-scrollable-main-stage">
      <g:FlowPanel addStyleNames="{style.pageContent}">
        <b:JQMButton text="" html="&lt;b&gt;Button&lt;/b&gt; with &lt;i&gt;HTML&lt;/i&gt; content"
          mini="true" inline="true" customIcon="loader" iconPos="RIGHT" />
        <b:JQMButtonPanel mini="true" inline="true" addStyleNames="{style.marginLeft10}">
          <b:widget> <g:Label text="Button" /> </b:widget>
          <b:widget> <g:Label text="Panel" /> </b:widget>
        </b:JQMButtonPanel>  
        <h:Div />
        <h:Span text="span text"/>
        <h:Div text="div text" />
        <jqm:JQMIcon iconNoDisc="false" iconShadow="false" iconAlt="false"
          text="Icon text" customIcon="loader" iconPos="LEFT" /> <!-- builtInIcon="AUDIO" -->
        <h:Div>
          <b:JQMButton text="Always Active Button in div" inline="true" mini="true"
            corners="false" alwaysActive="true" />
          <b:JQMButton text="Always Hover Button in div" inline="true" mini="true"
            corners="false" alwaysHover="true" ui:field="alwaysHoverBtn" />
          <b:JQMButton text="Toggle 'Always Hover' state" inline="true" mini="true"
            corners="false" builtInIcon="LEFT" ui:field="toggleAlwaysHoverBtn" />
        </h:Div>
        <f:JQMForm ui:field="form">
          <b:JQMButton text="Go To Test Page 2" href="#testPage2"
            transition="NONE" iconPos="RIGHT" builtInIcon="RIGHT"/>
          <g:FlowPanel>
            <b:JQMButton ui:field="page2AsDialog" text="Show Page 2 as Dialog (programmatic)"
              mini="true" inline="true" />
            <b:JQMButton text="Show Page 2 as Dialog (declarative)"
              mini="true" inline="true" dialog="true" href="#testPage2" />
            <b:JQMButton ui:field="page2RestoreRolePage" text="Restore Page 2 as page"
              mini="true" inline="true" />
            <b:JQMButton ui:field="page2RestoreRoleDialog" text="Restore Page 2 as dialog"
              mini="true" inline="true" />
          </g:FlowPanel>
          <e:JQMFilterable ui:field="btnsFltr" placeHolder="Filter buttons..." theme="a" /> 
          <b:JQMButtonGroup ui:field="btnGroup1" dataFilter="#{btnsFltr.getFilterId}">
            <b:JQMButton theme="a" text="A"/>
            <b:JQMButton theme="b" text="B"/>
            <b:JQMButton theme="c" text="C"/>
            <b:JQMButton theme="d" text="D"/>
            <b:JQMButton theme="e" text="E"/>
            <b:JQMButton theme="f" text="F"/>
          </b:JQMButtonGroup>
          <b:JQMButtonGroup orientation="HORIZONTAL" mini="true" corners="false">
            <b:JQMButton theme="a" text="A"/>
            <b:JQMButton theme="b" text="B"/>
          </b:JQMButtonGroup>
          <e:JQMCheckbox ui:field="cb1" name="cb1" text="Checkbox"/>
          <e:JQMCheckbox ui:field="cb2" name="cb2" text="Mini-Checkbox" mini="true" value="true"/>
          <e:JQMCheckbox ui:field="cb3" name="cb3" text="Themed Checkbox, no corners, inline" 
            theme="b" corners="false" inline="true" />
          <e:JQMCheckbox ui:field="cb4" name="cb4" text="Right Icon and No corners"
            corners="false" iconPos="RIGHT" />
          <b:JQMButton ui:field="checkboxInfoBtn" text="Show Checkboxes Info" />
          <e:JQMCheckset text="Mini Horizontal Checkset" orientation="HORIZONTAL" mini="true">
            <e:check><e:JQMCheckbox name="cb10" text="Check1" /></e:check>
            <e:check><e:JQMCheckbox name="cb11" text="Check2" /></e:check>
            <e:check><e:JQMCheckbox name="cb12" text="Check3" /></e:check>
          </e:JQMCheckset>
          <e:JQMCheckset text="Vertical Checkset"> <!-- iconPos="RIGHT" -->
            <e:check><e:JQMCheckbox name="cb20" text="Check1" /></e:check>
            <e:check><e:JQMCheckbox name="cb21" text="Check2" /></e:check>
          </e:JQMCheckset>
          <e:JQMEmail text="email" corners="false" clearButton="true" />
          <e:JQMFile text="File" corners="false" />
          <e:JQMFlip ui:field="flip"
            label1="Flip1" label2="Flip2" value1="flip1Value" value2="flip2Value" text="flipText"
            value="flip1Value" /> 
            <!-- theme="b" trackTheme="a" mini="true" textHidden="true" corners="false" -->
          <ly:JQMTable columns="4">
            <ly:cell addStyleNames="aaa bbb">
              <b:JQMButton ui:field="flipInfoBtn" text="Show Flip Info" mini="true" />
            </ly:cell>
            <ly:cell>
              <b:JQMButton ui:field="setFlipValBtn" text="Set Value to 'Flip2'" mini="true" />
            </ly:cell>
            <ly:cell>
              <b:JQMButton ui:field="setFlipNullBtn" text="Set Value to 'null'" mini="true" />
            </ly:cell>
            <ly:cell>
              <g:FlowPanel>
                <e:JQMText ui:field="flipTrackTheme" labelHidden="true" width="50px"
                  addStyleNames="{style.inlineBlock} {style.marginTopBottom0}" />
                <b:JQMButton ui:field="setFlipTrackTheme" text="Set Track Theme" mini="true" inline="true" />
              </g:FlowPanel>
            </ly:cell>
          </ly:JQMTable>

          <dt:JQMCalBox ui:field="datePicker" text="Date Picker"
            dateFormat="%A %m/%d/%y" value="2013-10-25" useClearButton="true" />
            <!-- useInline="true" lockInput="false" placeHolder="DayOfWeek mm/dd/yy" dialogLabel="Date Picker" 
              buttonIcon="gear" editable="false" 
              invalidateUnlockedInputOnBlur="false" iconNoDisc="true" iconAlt="true" /-->
          <b:JQMButton ui:field="datePickerGetValueBtn" text="Get Date Picker value"
            inline="true" mini="true" />
          <b:JQMButton ui:field="datePickerSetNullBtn" text="Set Date Picker value to Null"
            inline="true" mini="true" addStyleNames="{style.marginRight20}" />
          <h:Span text="Last onValueChange:" addStyleNames="{style.marginRight10}" />
          <h:Span ui:field="datePickerValueChanged" />  

          <e:JQMMonth ui:field="monthEd" text="monthText"/>
          <e:JQMNumber ui:field="numberEd" text="numberText"/>
          <e:JQMPassword ui:field="passwordEd" text="passwordText"/>
          <e:JQMSearch ui:field="searchEd" text="searchText"/>
          <e:JQMTelephone ui:field="phoneEd" text="telephoneText"/>
          <e:JQMText ui:field="text" text="Text (no corners)" corners="false" />
          <b:JQMButton builtInIcon="MINUS" ui:field="disableTextButton" text="Disable Text" inline="true" mini="true" />
          <b:JQMButton builtInIcon="PLUS" ui:field="enableTextButton" text="Enable Text" inline="true" mini="true" />

          <!-- TODO fix col/rows setter <e:JQMTextArea text="textAreaText" columns="40" rows="20"/>-->
          <e:JQMTextArea text="textAreaText"/>
          <e:JQMUrl ui:field="urlEd" text="urlText"/>

          <e:JQMRadioset ui:field="radio1" text="radiosetText" selectedValue="b">
            <!-- orientation="HORIZONTAL" mini="true" iconPos="RIGHT" theme="a" -->
            <e:radio>
              <e:JQMRadio text="radio_A_Text" value="a"/>
            </e:radio>
            <e:radio>
              <e:JQMRadio text="radio_B_Text" value="b"/>
            </e:radio>
            <e:radio>
              <e:JQMRadio text="radio_C_Text" value="c"/>
            </e:radio>
            <e:radio>
              <e:JQMRadio text="radio_D_Text" value="d"/>
            </e:radio>
          </e:JQMRadioset>
          <ly:JQMTable columns="3">
            <ly:cell>
              <b:JQMButton ui:field="radioInfoBtn" text="Show Radioset Info" />
            </ly:cell>
            <ly:cell>
              <b:JQMButton ui:field="setRadioValBtn" text="Set Value to 'c'" />
            </ly:cell>
            <ly:cell>
              <b:JQMButton ui:field="setRadioNullBtn" text="Set Value to 'null'" />
            </ly:cell>
          </ly:JQMTable>
          <e:JQMRadioset ui:field="gender" text="Gender"
            orientation="HORIZONTAL" labelHidden="true">
            <e:radio>
              <e:JQMRadio text="Male" value="M"/>
            </e:radio>
            <e:radio>
              <e:JQMRadio text="Female" value="F"/>
            </e:radio>
          </e:JQMRadioset>

          <h:Paragraph />
          <e:JQMSelect text="selectText" ui:field="select1" mini="true"
            builtInIcon="BARS" iconPos="LEFT" native="false" hidePlaceholderMenuItems="false">
            <!-- conNoDisc="true" iconAlt="true" -->
            <e:option>
              <e:JQMSelect.Option text="Select One..." placeholder="true" />
            </e:option>
            <e:option>
              <e:JQMSelect.Option text="option1Text" value="option1value"/>
            </e:option>
            <e:option>
              <e:JQMSelect.Option text="option2Text" value="option2value"/>
            </e:option>
            <e:option>
              <e:JQMSelect.Option text="option3Text" value="option3value" disabled="true" />
            </e:option>
            <e:option>
              <e:JQMSelect.Option text="option4Text" value="option4value" selected="true" />
            </e:option>
          </e:JQMSelect>
          <g:FlowPanel>
            <b:JQMButton ui:field="getSelectValBtn" text="Get Dropdown Value" inline="true" mini="true" />
            <b:JQMButton ui:field="setSelectValBtn" text="Set SelectedIndex to 1" inline="true" mini="true" />
            <b:JQMButton ui:field="setSelectValNullBtn" text="Set Dropdown Value to null" inline="true" mini="true" />
            <b:JQMButton ui:field="addSelectItemsBtn" text="Add Dropdown Items" inline="true" mini="true" />
          </g:FlowPanel>
          <h:Paragraph />
          <e:JQMSelectWithIcons ui:field="ddIcons" text="Dropdown with icons" 
            builtInIcon="BARS" selectInline="true">
            <e:option><e:JQMSelect.Option text="Bars" value="1" icon="BARS" /></e:option>
            <e:option><e:JQMSelect.Option text="Alert" value="2" icon="ALERT" /></e:option>
            <e:option><e:JQMSelect.Option text="Audio" value="3" icon="AUDIO" /></e:option>
            <e:option><e:JQMSelect.Option text="Loader" value="4" customIcon="loader" /></e:option>
          </e:JQMSelectWithIcons>
          <h:Paragraph />
          <e:JQMSelect text="Dropdown with multiple select" mulitple="true" native="false"
            closeText="Discard" selectInline="true">
            <e:option><e:JQMSelect.Option text="One" value="1" /></e:option>
            <e:option><e:JQMSelect.Option text="Two" value="2" /></e:option>
            <e:option><e:JQMSelect.Option text="Three" value="3" /></e:option>
            <e:option><e:JQMSelect.Option text="Four" value="4" /></e:option>
            <e:option><e:JQMSelect.Option text="Five" value="5" /></e:option>
          </e:JQMSelect>
          <h:Paragraph />
          <h:Heading.H4 text="Filter Select/Dropdown"
            addStyleNames="{style.inlineBlock} {style.marginRight10}" />
          <b:JQMButton ui:field="addNumsBtn" text="Add numbers" inline="true" />
          <e:JQMFilterable ui:field="numsFltr" />
          <e:JQMSelect ui:field="numsList" text="" labelHidden="true" native="false"
            dataFilter="#{numsFltr.getFilterId}" >
            <e:option><e:JQMSelect.Option text="Zero" value="0" /></e:option>
            <e:option><e:JQMSelect.Option text="One" value="1" /></e:option>
            <e:option><e:JQMSelect.Option text="Two" value="2" /></e:option>
            <e:option><e:JQMSelect.Option text="Three" value="3" /></e:option>
            <e:option><e:JQMSelect.Option text="Four" value="4" /></e:option>
            <e:option><e:JQMSelect.Option text="Five" value="5" /></e:option>
            <e:option><e:JQMSelect.Option text="Six" value="6" /></e:option>
            <e:option><e:JQMSelect.Option text="Seven" value="7" /></e:option>
            <e:option><e:JQMSelect.Option text="Eight" value="8" /></e:option>
            <e:option><e:JQMSelect.Option text="Nine" value="9" /></e:option>
          </e:JQMSelect>
          <h:Paragraph />
          <e:JQMSelectFilterable ui:field="selectFilterable" text="Select Filterable (startWith)"
            menuStyleNames="aaa bbb ccc">
            <e:option><e:JQMSelect.Option text="San Francisco" value="SFO" /></e:option>
            <e:option><e:JQMSelect.Option text="Los Angeles" value="LAX" /></e:option>
            <e:option><e:JQMSelect.Option text="Vancouver" value="YVR" /></e:option>
            <e:option><e:JQMSelect.Option text="Toronto" value="YYZ" /></e:option>
            <e:option><e:JQMSelect.Option text="Dnepropetrovsk" value="DNK" /></e:option>
            <e:option><e:JQMSelect.Option text="San Jose" value="SJC" filterText="San Jose, SJC" /></e:option>
            <e:option><e:JQMSelect.Option text="Dublin" value="DUB" /></e:option>
          </e:JQMSelectFilterable>
          <h:Paragraph />
          <h:Heading.H4 text="Filter Select/Dropdown Reveal"
            addStyleNames="{style.inlineBlock} {style.marginRight10}" />
          <g:FlowPanel addStyleNames="{style.inlineBlock}">
            <b:JQMButton ui:field="addFruitsBtn" text="Add fruits" inline="true" mini="true" />
            <b:JQMButton ui:field="fruitsGetSearchTextBtn"
              text="Get Search Text" mini="true" inline="true" />
            <g:FlowPanel addStyleNames="{style.inlineBlock}" >
              <e:JQMText ui:field="fruitsSetSearchTextEd" labelHidden="true" />
            </g:FlowPanel>
            <b:JQMButton ui:field="fruitsSetSearchTextBtn"
                text="Set Search Text" mini="true" inline="true" />
          </g:FlowPanel>
          <l:JQMList.Unordered ui:field="fruitsList" inset="true"
            filterable="true" filterReveal="true" filterPlaceholder="Search fruits...">
            <l:item><l:JQMListItem checkBox="LEFT" text="Apple" /></l:item>
            <l:item><l:JQMListItem checkBox="LEFT" text="Banana" /></l:item>
            <l:item><l:JQMListItem checkBox="LEFT" text="Cherry" /></l:item>
            <l:item><l:JQMListItem checkBox="LEFT" text="Cranberry" /></l:item>
            <l:item><l:JQMListItem checkBox="LEFT" text="Grape" /></l:item>
            <l:item><l:JQMListItem checkBox="LEFT" text="Orange" /></l:item>
          </l:JQMList.Unordered>
          <h:Paragraph />
          <h:Heading.H4 text="City Search" addStyleNames="{style.inlineBlock} {style.marginRight10}" />
          <l:JQMList.Unordered ui:field="cityList" inset="true"
            filterable="true" filterReveal="true" filterPlaceholder="Find a city...">
          </l:JQMList.Unordered>
          <h:Paragraph />
          <h:Paragraph addStyleNames="{style.bevel}" />

          <e:JQMSlider min="5.0" max="8.0" labelText="sliderLabelText" ui:field="slider" />
            <!-- highlight="true" mini="true" theme="a" /-->
          <g:FlowPanel>
            <b:JQMButton builtInIcon="MINUS" ui:field="disableSliderButton" text="Disable Slider" inline="true" mini="true" />
            <b:JQMButton builtInIcon="PLUS" ui:field="enableSliderButton" text="Enable Slider" inline="true" mini="true" />
            <b:JQMButton ui:field="getSliderValueBtn" text="Get Slider Value" inline="true" mini="true" />
            <b:JQMButton ui:field="setSliderNullBtn" text="Set Slider Value to null" inline="true" mini="true" />
            <b:JQMButton ui:field="setSliderMinBtn" text="Set Slider Value to min" inline="true" mini="true" />
            <b:JQMButton ui:field="setSliderMinTo6Btn" text="Set Slider Min to 6" inline="true" mini="true" />
            <b:JQMButton ui:field="setSliderStepBtn" text="Set Slider Step to 0.5" inline="true" mini="true" />
          </g:FlowPanel>
          <e:JQMRangeSlider ui:field="rangeSlider" text="Range Slider"
            minInt="20" maxInt="80" loValue="30.0" hiValue="70.0"
            step="0.5" highlight="true" mini="false" theme="a" trackTheme="b" labelHidden="false" />
          <g:FlowPanel>
            <b:JQMButton builtInIcon="MINUS" ui:field="disableRangeSliderBtn" text="Disable Range Slider" inline="true" mini="true" />
            <b:JQMButton builtInIcon="PLUS" ui:field="enableRangeSliderBtn" text="Enable Range Slider" inline="true" mini="true" />
            <b:JQMButton ui:field="rangeSliderGetValuesBtn" text="Get Values" inline="true" mini="true" />
            <b:JQMButton ui:field="setRangeSliderNullBtn" text="Set Range Slider Values to null" inline="true" mini="true" />
          </g:FlowPanel>
          <h:Paragraph />
          <t:JQMNavBar iconPos="LEFT">
            <t:button><b:JQMButton ui:field="navBarBtn1" text="Button 1"/></t:button>
            <t:button><b:JQMButton text="Button 2" builtInIcon="STAR"/></t:button>
            <t:buttonActivePersist><b:JQMButton text="Button 3"/></t:buttonActivePersist>
            <t:button><b:JQMButton text="Button 4"/></t:button>
            <t:button><b:JQMButton text="Button 5"/></t:button>
          </t:JQMNavBar>
          <h:Paragraph />
          <h:Span text="Navbar with last clicked highlighting (icons pos is RIGHT)" />
          <t:JQMNavBar iconPos="RIGHT" highlightLastClicked="true">
            <t:button><b:JQMButton text="Button 1"/></t:button>
            <t:button><b:JQMButton text="Button 2" builtInIcon="GEAR" /></t:button>
            <t:button><b:JQMButton text="Button 3"/></t:button>
            <t:button><b:JQMButton text="Button 4"/></t:button>
            <t:buttonActivePersist><b:JQMButton text="Button 5"/></t:buttonActivePersist>
          </t:JQMNavBar>
          <h:Paragraph />
          <h:Paragraph />
          <h:Heading.H3 text="Tabs widget with headers as buttons"
            addStyleNames="{style.headerImportantColor}" />
          <t:JQMTabs iconPos="RIGHT" activeIndex="1" heightStyle="AUTO">
            <t:button>
              <b:JQMButton text="Tab1"/>
            </t:button>
            <t:button>
              <b:JQMButton text="Tab2" builtInIcon="GEAR" />
            </t:button>
            <t:button>
              <b:JQMButton text="Tab3"/>
            </t:button>
            <t:content>
              <g:FlowPanel addStyleNames="ui-body-inherit ui-content"> <!-- styles are OPTIONAL -->
                <h:Heading.H3 text="Tab1 Content" />
              </g:FlowPanel>
            </t:content>
            <t:content>
              <g:FlowPanel addStyleNames="ui-body-inherit ui-content">
                <h:Heading.H3 text="Tab2 Content" />
                <h:Paragraph />
                <h:Heading.H4 text="Foo1" />
                <h:Heading.H4 text="Foo2" />
                <h:Heading.H4 text="Foo3" />
              </g:FlowPanel>
            </t:content>
            <t:content>
              <g:FlowPanel addStyleNames="ui-body-inherit ui-content">
                <h:Heading.H3 text="Tab3 Content" />
              </g:FlowPanel>
            </t:content>
          </t:JQMTabs>
          <h:Paragraph />
          <h:Paragraph />
          <h:Heading.H3 text="Tabs widget with headers as listview"
            addStyleNames="{style.headerImportantColor}" />
          <t:JQMTabs collapsible="true" activeIndex="1">
            <!-- tabDisabled="[2]" collapseAll="true" -->
            <t:listitem>
              <l:JQMListItem text="Tab1" />
            </t:listitem>
            <t:listitem>
              <l:JQMListItem text="Tab2" dataIcon="GEAR" />
            </t:listitem>
            <t:listitem>
              <l:JQMListItem text="Tab3" />
            </t:listitem>
            <t:content>
              <g:FlowPanel addStyleNames="ui-body-inherit ui-content"> <!-- styles are OPTIONAL -->
                <h:Heading.H3 text="Tab1 Content" />
              </g:FlowPanel>
            </t:content>
            <t:content>
              <g:FlowPanel addStyleNames="ui-body-inherit ui-content">
                <h:Heading.H3 text="Tab2 Content" />
              </g:FlowPanel>
